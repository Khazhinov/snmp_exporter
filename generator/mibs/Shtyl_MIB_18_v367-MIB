				INDEX   		{ termoRelayIndex }
				::= { termoRelayTable 1 }

				TermoRelayEntry ::=
				SEQUENCE {
				  termoRelayIndex			INTEGER,
	  			  termoRelayMode			OCTET STRING,
	  			  termoRelayHighTemperature		OCTET STRING,
	  			  termoRelayLowTemperature		OCTET STRING,
	  			  termoRelayMeasuredTemperature		OCTET STRING,
	  			  termoRelayStatus			INTEGER				  
				}
			
					termoRelayIndex	OBJECT-TYPE
					SYNTAX 			INTEGER(1..32)
					MAX-ACCESS 		not-accessible
					STATUS 			current
					DESCRIPTION 		"Index of Thermo Relays."
					::=  			{ termoRelayEntry 1}
--Режим работы термореле  охлаждение\нагрев\норма
					termoRelayMode  OBJECT-TYPE
					SYNTAX 			OCTET STRING (SIZE (0..255))
					MAX-ACCESS 		read-only
					STATUS 			current
					DESCRIPTION 		"Mode of Thermo Relay"
					::=  			{ termoRelayEntry 2 }
--Верхняя температура термореле
