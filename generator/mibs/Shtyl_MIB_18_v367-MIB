					SYNTAX 			OCTET STRING (SIZE (0..255))
					MAX-ACCESS 		read-only
					STATUS 			current
					DESCRIPTION 		"Invertor Output current."
					::=  			{ invEntry 2 }
--Выходное напряжение инвертора
					invOutputVoltage OBJECT-TYPE
					SYNTAX 			OCTET STRING (SIZE (0..255))
					MAX-ACCESS 		read-only
					STATUS 			current
					DESCRIPTION 		"Invertor output voltage."
					::=  			{ invEntry 3 }
--Входное напряжение инвертора
					invInputVoltage OBJECT-TYPE
					SYNTAX 			OCTET STRING (SIZE (0..255))
					MAX-ACCESS 		read-only
					STATUS 			current
					DESCRIPTION 		"Invertor input voltage."
					::=  			{ invEntry 4 }
--Частота выходного напряжения инвертора
					invFrequency OBJECT-TYPE
					SYNTAX 			OCTET STRING (SIZE (0..255))
					MAX-ACCESS 		read-only
					STATUS 			current
					DESCRIPTION 		"Invertor frequency output voltage."
					::=  			{ invEntry 5 }
--Частота входного напряжения инвертора
					invInputFrequency OBJECT-TYPE
					SYNTAX 			OCTET STRING (SIZE (0..255))
					MAX-ACCESS 		read-only
					STATUS 			current
					DESCRIPTION 		"Invertor input frequency."
					::=  			{ invEntry 6 }
--Номинальное состояние байпаса
					invBypassNominalState OBJECT-TYPE
					SYNTAX 			OCTET STRING (SIZE (0..255))
					MAX-ACCESS 		read-only
					STATUS 			current
					DESCRIPTION 		"Bypass nominal state"
					::=  			{ invEntry 7 }
--текущее состояние байпаса
