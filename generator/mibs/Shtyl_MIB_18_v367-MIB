				INDEX   		{ invIndex }
				::= { invTable 1 }

				InvEntry ::=
				SEQUENCE {
				  invIndex		INTEGER,
	  			  invOutputCurrent	OCTET STRING,
	  			  invOutputVoltage	OCTET STRING,
	  			  invInputVoltage	OCTET STRING,
				  invFrequency		OCTET STRING,
				  invInputFrequency	OCTET STRING,
				  invBypassNominalState	OCTET STRING,
				  invBypassState	OCTET STRING,
				  invTemperature	OCTET STRING,
				  invGroup		INTEGER,
				  invAlarm		OCTET STRING
				}
			
					invIndex	OBJECT-TYPE
					SYNTAX 			INTEGER(1..127)
					MAX-ACCESS 		not-accessible
					STATUS 			current
					DESCRIPTION 		"Index of Invertor."
					::=  			{ invEntry 1}
--Выходной ток инвертора
