    ::= { HostIPConfig 5 }


---------------------------
-- 'CommonConfig' SubDirectory --

GlobalSettings    OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    		" Global settings for device (bitfield):
			bits 31..3 - reserved; 
			bit 2 - external XCK output ( 0 - disabled, 1 - enabled ); 
			bits 1..0 - main RS-232 speed ( 0 - 57600, 1 - 9600, 2 - 19200, 3 - 57600 ) "
    ::= { CommonConfig 1 }

Net_ID    OBJECT-TYPE
    SYNTAX  INTEGER (0..65535)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    		" RSTTL network ID "
    ::= { CommonConfig 2 }

RSTTL_mode    OBJECT-TYPE
    SYNTAX  INTEGER (0..65535)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    		" RSTTL work mode (bitfield): 
			bits 15..1 - reserved; 
			bit 0 - RSTTL mode: 0 - master; 1 - slave "
    ::= { CommonConfig 3 }

ConsoleTimeout    OBJECT-TYPE
    SYNTAX  INTEGER (0..255)
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    		" Timeout for control inactivity of console users (0..59) - (1..60 min) "
    ::= { CommonConfig 4 }


---------------------------
-- 'STM1Config' SubDirectory --

STM1CfgTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF STM1CfgEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"The STM-1 Tributaries Configuration table."
            ::= { STM1Config 1 }

			stm1CfgEntry OBJECT-TYPE
	            SYNTAX  STM1CfgEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the STM1 Tributaries Configuration table."
	            INDEX { STM1CfgIndex }
	            ::= { STM1CfgTable 1 }

			STM1CfgEntry ::= SEQUENCE {
				STM1CfgIndex
					INTEGER,
				STM1CfgConfig
					INTEGER
				}

			STM1CfgIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each interface.  Its value ranges between 1 and number of available STM1 ports."
	            ::= { stm1CfgEntry 1 }
			
			STM1CfgConfig    OBJECT-TYPE
	            SYNTAX  INTEGER (0..65535)
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" Configuration options of STM1 interface (bitfield):
						bits 15..3 - reserved;
						bits 2..1 - Loopback mode: 0 - off, 1 - Local (system side), 2 - Remote (line side), 3 - reserved
						bit 0 - Power: 0 - off, 1 - on "
	            ::= { stm1CfgEntry 2 }

STM1_VC12_SwTable    OBJECT-TYPE
	SYNTAX  SEQUENCE OF STM1_VC12CfgEntry
	ACCESS  not-accessible
	STATUS  mandatory
	DESCRIPTION
            "The STM-1 VC-12 mapping table."
	::= { STM1Config 2 }

	stm1_VC12CfgEntry OBJECT-TYPE
		SYNTAX  STM1CfgEntry
		ACCESS  not-accessible
		STATUS  mandatory
		DESCRIPTION
	            "An entry in the STM1 VC-12 mapping table."
		INDEX { STM1_VC12CfgIndex }
		::= { STM1_VC12_SwTable 1 }

	STM1_VC12CfgEntry ::= SEQUENCE {
		STM1_VC12CfgIndex
			INTEGER,
		STM1_VC12CfgConfig
			INTEGER
		}

	STM1_VC12CfgIndex    OBJECT-TYPE
		SYNTAX  INTEGER
		ACCESS  read-only
		STATUS  mandatory
		DESCRIPTION
	            "A unique value for each VC12.  First 63 entries for 1-st STM-1 interface, second 63 entries for 2nd STM-1."
		::= { stm1_VC12CfgEntry 1 }
	
	STM1_VC12CfgConfig    OBJECT-TYPE
		SYNTAX  INTEGER (0..65535)
		ACCESS  read-write
		STATUS  mandatory
		DESCRIPTION
	            " Configuration options of E1 interface (bitfield):
				bit 15 - VC12 tract: 0 - free, 1 - busy;
				bit 14..13 - Type of connection: 00 - another VC12, 01 - E1 port, 10 - Ethernet, 11 - reserve;
				bit 12 - SNCP status: 0 - main VC12, 1 - reserved VC12 (for VC12 connected to E1 ports); 
				bit 11..7 - reserved; 
				bit 6 - STM1 number of destination VC12; 
				bit 5..0 - destination: VC12 (0..63); E1 port (0..15); WAN port (0..1) "
		::= { stm1_VC12CfgEntry 2 }

MSP_ReservationCfg    OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    		" Multiplex Section Protection settings (bitfield):
			 bits 7..3 - reserved; 
			 bit 2 - MSP revert mode: 0 - Non-revertive, 1 - revertive
			 bit 1 - MSP direction mode: 0 - unidirectional, 1 - bidirectional
			 bit 0 - MSP 1+1 Reservation: 0 - disabled, 1 - enabled (1st STM-1 is main, 2nd STM-1 is reserve) "
    ::= { STM1Config 3 }

MSP_WTR_time    OBJECT-TYPE
    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    		" MSP wait to restore time (60..720 seconds)"
    ::= { STM1Config 4 }

---------------------------
-- 'E1Config' SubDirectory --

E1CfgTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF E1CfgEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"The E1 Tributaries Configuration table."
            ::= { E1Config 1 }

			e1CfgEntry OBJECT-TYPE
	            SYNTAX  E1StatusEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the E1 Tributaries Configuration table."
	            INDEX { E1CfgIndex }
	            ::= { E1CfgTable 1 }

			E1CfgEntry ::= SEQUENCE {
				E1CfgIndex
					INTEGER,
				E1CfgConfig
					INTEGER
				}

			E1CfgIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each interface.  Its value ranges between 1 and number of available E1 ports."
	            ::= { e1CfgEntry 1 }
			
			E1CfgConfig    OBJECT-TYPE
	            SYNTAX  INTEGER (0..255)
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" Configuration options of E1 interface (bitfield):
						bit 7 - POWER:    0 - disabled; 1 - enabled;
						bit 6 - SNCP protection:    0 - disabled; 1 - enabled;
						bit 5 - reserved; 
						bit 4 - Remote loop (Framer data is looped): 0 - disabled; 1 - enabled; 
						bit 3 - Local loop (line side looped): 0 - disabled; 1 - enabled; 
						bits 2..0 - reserved "
	            ::= { e1CfgEntry 2 }

---------------------------
-- 'EthConfig' SubDirectory --

LAN_CfgTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF LAN_EthCfgEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"The LAN (Copper Ethernet) Ports Configuration table."
            ::= { EthConfig 1 }

			lan_EthCfgEntry OBJECT-TYPE
	            SYNTAX  LAN_EthCfgEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the Copper Ethernet Ports Configuration table."
	            INDEX { LAN_EthCfgIndex }
	            ::= { LAN_CfgTable 1 }

			LAN_EthCfgEntry ::= SEQUENCE {
				LAN_EthCfgIndex
					INTEGER,
				LAN_EthCfgConfig
					INTEGER
				}

			LAN_EthCfgIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each interface.  Its value ranges between 1 and number of LAN ports."
	            ::= { lan_EthCfgEntry 1 }
			
			LAN_EthCfgConfig    OBJECT-TYPE
	            SYNTAX  INTEGER (0..65535)
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" Configuration options of LAN port (bitfield):
						bit 15 - POWER:    0 - OFF (power down); 1 - ON (normal operation);
						bit 14 - Auto Negotiation:    0 - disabled; 1 - enabled;
						bit 13 - Auto MDI/MDI-X:    0 - disabled; 1 - enabled;
						bit 12 - Force MDI-X, valid only if bit10=0 (no auto MDI/MDI-X)):    0 - force MDI; 1 - force MDI-X;
						bits 11..3 - reserved;
						bit 2 - Duplex force, valid only if bit14=0 (no AN):   0 - force half duplex; 1 - force full duplex;
						bits 1..0 - Speed force, valid only if bit14=0 (no AN):
							00 - force 10Mbps; 01 - force 100Mbps; 10, 11 - reserved "
	            ::= { lan_EthCfgEntry 2 }

WAN_CfgTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF WAN_EthCfgEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"The WAN (Ethernet over STM) Ports Configuration table."
            ::= { EthConfig 2 }

			wan_EthCfgEntry OBJECT-TYPE
	            SYNTAX  WAN_EthCfgEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the WAN Ports Configuration table."
	            INDEX { WAN_EthCfgIndex }
	            ::= { WAN_CfgTable 1 }

			WAN_EthCfgEntry ::= SEQUENCE {
				WAN_EthCfgIndex
					INTEGER,
				WAN_EthCfgConfig
					INTEGER
				}

			WAN_EthCfgIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each interface.  Its value ranges between 1 and number of WAN ports."
	            ::= { wan_EthCfgEntry 1 }
			
			WAN_EthCfgConfig    OBJECT-TYPE
	            SYNTAX  INTEGER (0..65535)
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" Configuration options of WAN port (bitfield):
						bits 15..1 - reserved;
						bit 0 - Mode: 0 - non LCAS, 1 - LCAS "
	            ::= { wan_EthCfgEntry 2 }


---------------------------
-- 'SyncConfig' SubDirectory --

SyncMode    OBJECT-TYPE
	SYNTAX  INTEGER (0..255)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	        " Global synchronization settings (bitfield):
			bits 7..2 - reserved;
			bit 1 - QL en mode (0 - QL disabled, 1 - QL enabled);
			bit 0 - System sync mode (0 - automatic sync selection, 1 - Forced freerun mode) "
	::= { SyncConfig 1 }

Hold_off_time    OBJECT-TYPE
	SYNTAX  INTEGER (0..255)
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
			"Hold off time for sync input (value from 3 to 18 in 100msec units) (300..1800msec)."
	::= { SyncConfig 2 }

Wait_to_restore_time  OBJECT-TYPE
	SYNTAX 	INTEGER (0..255)
	ACCESS  read-write
	STATUS 	mandatory
	DESCRIPTION 
			"Wait to restore time for sync input (0..12 minutes)."
	::= { SyncConfig 3 }

SyncIn_CfgTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF SyncInCfgEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"Synchronization input ports configuration table."
            ::= { SyncConfig 4 }

			syncInCfgEntry OBJECT-TYPE
	            SYNTAX  SyncInCfgEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the Sync Input Ports Configuration table."
	            INDEX { SyncInCfgIndex }
	            ::= { SyncIn_CfgTable 1 }

			SyncInCfgEntry ::= SEQUENCE {
				SyncInCfgIndex
					INTEGER,
				SyncInCfgConfig
					INTEGER
				}

			SyncInCfgIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each sync input. It show priority of input. Its value ranges between 1 and 4."
	            ::= { syncInCfgEntry 1 }
			
			SyncInCfgConfig    OBJECT-TYPE
	            SYNTAX  INTEGER (0..65535)
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" Configuration options of LAN port (bitfield):
						bit 15 - SyncIn mode (0 - disabled, 1 - enabled);
						bits 14..13 - reserved;
						bit 12 - sync_in QL mode (0 - auto, 1 - forced);
						bits 11..8 - forced QL value (used only in forced mode (bit12=1)) (1-DNU, 2-SEC, 3-SSU_B, 4-SSU_A, 5-PRC);
						bits 7..0 - sync input:
							0..1 - STM-1 RCLK sources;
							32..47 - E1 RCLK sources; 
							64 - External sync input "
	            ::= { syncInCfgEntry 2 }

