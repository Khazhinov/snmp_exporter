STM1StatusTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF STM1StatusEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"The STM1 status table."
            ::= { STM1State 1 }

			stm1StatusEntry OBJECT-TYPE
	            SYNTAX  STM1StatusEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the STM1 status table."
	            INDEX { STM1StatusIndex }
	            ::= { STM1StatusTable 1 }

			STM1StatusEntry ::= SEQUENCE {
				STM1StatusIndex
					INTEGER,
				STM1StatusStatus
					INTEGER,
				MSP_path_state
					INTEGER
				}

			STM1StatusIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each entry.  Its value ranges between 1 and number of available STM1 ports."
	            ::= { stm1StatusEntry 1 }
			
			STM1StatusStatus    OBJECT-TYPE
	            SYNTAX  INTEGER 
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" Current status flags for STM1 port (bitfield):
						 bits 15..12 - reserved; 
						 bit 11 - LOM (POH alarm); 
						 bit 10 - HP-UNEQ (POH alarm); 
						 bit 9 - VC-AIS (POH alarm);
						 bit 8 - Loss Of Pointer (POH alarm); 
						 bit 7 - AU-AIS (POH alarm);
						 bit 6 - DEG (MS alarm);
						 bit 5 - EXC (MS alarm); 
						 bit 4 - MS-RDI (MS alarm);
						 bit 3 - MS-AIS (MS alarm); 
						 bit 2 - LOF (RS alarm); 
						 bit 1 - LOS (no signal from SFP); 
						 bit 0 - no SFP "
	            ::= { stm1StatusEntry 2 }

			MSP_path_state    OBJECT-TYPE
				SYNTAX  INTEGER {
					NORMAL (0),
					SIGNAL_FAIL (1),
					SIGNAL_DEGRADE (2)
				}
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" State of MSP path (0 - Normal; 1 - Signal Fail; 2 - Signal Degrade "
	            ::= { stm1StatusEntry 3 }


