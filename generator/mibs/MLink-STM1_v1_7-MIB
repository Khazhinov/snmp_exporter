    SYNTAX  INTEGER
    ACCESS  read-write
    STATUS  mandatory
    DESCRIPTION
    		"Bridge ID priority (0..61440 in step of 4096)"
    ::= { STPConfig 5 }

STP_PortCfgTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF STP_PortCfgEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"Spanning tree port configuration table"
            ::= { STPConfig 6 }

			stp_PortCfgEntry OBJECT-TYPE
	            SYNTAX  STP_PortCfgEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the Spanning tree port configuration table."
	            INDEX { STP_PortCfgIndex }
	            ::= { STP_PortCfgTable 1 }

			STP_PortCfgEntry ::= SEQUENCE {
				STP_PortCfgIndex
					INTEGER,
				STP_PortCfgConfig
					INTEGER,
				STP_PortCfgID_Pri
					INTEGER,
				STP_PortCfgCost
					INTEGER
				}

			STP_PortCfgIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each interface.  Its value ranges between 1 and number of STP ports."
	            ::= { stp_PortCfgEntry 1 }
			
			STP_PortCfgConfig    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" Configuration options of spanning tree port (bitfield):
						bit 7 - State: 0 - disabled; 1 - enabled; 
						bit 6 - PortFast: 0 - disabled; 1 - enabled; 
						bit 5 - Auto PortFast: 0 - disabled; 1 - enabled; Valid only when PortFast(bit6)=1;  
						bits 4..0 - reserved "
	            ::= { stp_PortCfgEntry 2 }

			STP_PortCfgID_Pri    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" Port ID priority (0..240 in step of 16) "
	            ::= { stp_PortCfgEntry 3 }

			STP_PortCfgCost    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" Port cost (1..200000000) "
	            ::= { stp_PortCfgEntry 4 }

---------------------------
-- 'VLANConfig' SubDirectory --

VLAN_PortCfgTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF VLAN_PortCfgEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"VLAN port configuration table"
            ::= { VLANConfig 1 }

			vlan_PortCfgEntry OBJECT-TYPE
	            SYNTAX  VLAN_PortCfgEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the VLAN port configuration table."
	            INDEX { VLAN_PortCfgIndex }
	            ::= { VLAN_PortCfgTable 1 }

			VLAN_PortCfgEntry ::= SEQUENCE {
				VLAN_PortCfgIndex
					INTEGER,
				VLAN_PortCfgDefVID
					INTEGER,
				VLAN_PortCfgConfig
					INTEGER
				}

			VLAN_PortCfgIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each interface.  Its value ranges between 1 and number of Ethernet ports."
	            ::= { vlan_PortCfgEntry 1 }
			
			VLAN_PortCfgDefVID    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" Default VLAN ID of port (1..4095) "
	            ::= { vlan_PortCfgEntry 2 }

			VLAN_PortCfgConfig    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" VLAN Configuration options of port (bitfield):
						bits 15..3 - reserved; 
						bits 2..1 - 802.1Q mode: 0 - disabled; 1 - bypass; 2 - check; 3 - secure; 
						bit 0 - force ingress to default VID: 0 - disabled; 1 - enabled "
	            ::= { vlan_PortCfgEntry 3 }

VLAN_Table 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF VLAN_TableEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		" VLAN table "
            ::= { VLANConfig 2 }

			vlan_TableEntry OBJECT-TYPE
	            SYNTAX  VLAN_TableEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the VLAN table."
	            INDEX { VLAN_TableIndex }
	            ::= { VLAN_Table 1 }

			VLAN_TableEntry ::= SEQUENCE {
				VLAN_TableIndex
					INTEGER,
				VLAN_TableVID
					INTEGER,
				VLAN_TableMember
					INTEGER
				}

			VLAN_TableIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each entry.  Its value ranges between 1 and 256."
	            ::= { vlan_TableEntry 1 }
			
			VLAN_TableVID    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" VLAN Table entry VID (bitfield):
						bit 15 - mode (0 - invalid entry, 1 - valid entry); 
						bits 14..2 - reserved; 
						bits 11..0 - VLAN ID (1..4095) "
	            ::= { vlan_TableEntry 2 }

			VLAN_TableMember    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" VLAN membership of ports (bitfield):
						bits 15..10 - reserved; 
						bits 9..8 - option for port 5; 
						bits 7..6 - option for port 4; 
						bits 5..4 - option for port 3; 
						bits 3..2 - option for port 2; 
						bits 1..0 - option for port 1; 
						 Member options: 0 - member, unmodified; 1 - member, untagged; 2 - member, tagged; 3 - not member "
	            ::= { vlan_TableEntry 3 }

---------------------------
-- 'QoSConfig' SubDirectory --

Tag_Pri_mapping    OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS  read-write
	STATUS  mandatory
	DESCRIPTION
	        " 802.1p priority mapping (bitfield):
			bits 15..14 - class for Pri=7; 
			bits 13..12 - class for Pri=6; 
			bits 11..10 - class for Pri=5; 
			bits 9..8 - class for Pri=4; 
			bits 7..6 - class for Pri=3; 
			bits 5..4 - class for Pri=2; 
			bits 3..2 - class for Pri=1; 
			bits 1..0 - class for Pri=0; "
	::= { QoSConfig 1 }

DSCP_Pri_mapping 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF DSCP_mapEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		" DSCP priority mapping table. It defines class of service for different DSCP values "
            ::= { QoSConfig 2 }

			dscp_mapEntry OBJECT-TYPE
	            SYNTAX  DSCP_mapEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the DSCP mapping table."
	            INDEX { DSCP_mapIndex }
	            ::= { DSCP_Pri_mapping 1 }

			DSCP_mapEntry ::= SEQUENCE {
				DSCP_mapIndex
					INTEGER,
				DSCP_mapValue
					INTEGER
				}

			DSCP_mapIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each entry. Its value ranges between 1 and 8."
	            ::= { dscp_mapEntry 1 }
			
			DSCP_mapValue    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" bitX,X+1 - class of service (0..3) (0-lowest)
						   [0] - bits0,1 - IP_PRI 0; bits2,3 - IP_PRI 1;... ; bits14,15 - IP_PRI 7; 
						   ...
						   [7] - bits0,1 - IP_PRI 56; bits2,3 - IP_PRI 57;... ; bits14,15 - IP_PRI 63 "
	            ::= { dscp_mapEntry 2 }

QoS_PortCfgTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF QoS_PortCfgEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"QoS port configuration table"
            ::= { QoSConfig 3 }

			qos_PortCfgEntry OBJECT-TYPE
	            SYNTAX  QoS_PortCfgEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the QoS port configuration table."
	            INDEX { QoS_PortCfgIndex }
	            ::= { QoS_PortCfgTable 1 }

			QoS_PortCfgEntry ::= SEQUENCE {
				QoS_PortCfgIndex
					INTEGER,
				QoS_PortCfgConfig
					INTEGER,
				QoS_PortCfgTagRemap
					INTEGER
				}

			QoS_PortCfgIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each interface.  Its value ranges between 1 and number of Ethernet ports."
	            ::= { qos_PortCfgEntry 1 }
			
			QoS_PortCfgConfig    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
						" QoS configuration of CPU port (bitfield):
						bits 15..7 - reserved; 
						bit6 - scheduling mode (0-Weighted Round Robin, 1-Strict)
						bits5..3 - port default priority (0..7)
						bit2 - Priority for tagged IP frames (0-IP, 1-tag)
						bit1 - IP QoS (DSCP) enable (0-dont use IP priority, 1-use IP priority)
						bit0 - 802.1p QoS enable (0-dont use tag priority, 1-use tag priority) "
	            ::= { qos_PortCfgEntry 2 }

			QoS_PortCfgTagRemap    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-write
	            STATUS  mandatory
	            DESCRIPTION
	            		" 802.1p tag priority remap options of port (bitfield):
						bit31 - reserved,   bits30..28 - Pri value for tag Pri=7; 
						bit27 - reserved,   bits26..24 - Pri value for tag Pri=6; 
						...
						bit3 - reserved,   bits2..0 - Pri value for tag Pri=0 "
	            ::= { qos_PortCfgEntry 3 }


