-- 'commands' Directory --
--------------------------

ApplyCmd  OBJECT-TYPE
	SYNTAX  INTEGER { Set (1) }
	ACCESS     read-write
	STATUS     mandatory
	DESCRIPTION 
		"Command for apply new settings."
	::= { commands 1 }

SaveCmd  OBJECT-TYPE
	SYNTAX  INTEGER { Set (1) }
	ACCESS     read-write
	STATUS     mandatory
	DESCRIPTION 
		"Command for save new settings."
	::= { commands 2 }

RestoreCmd  OBJECT-TYPE
	SYNTAX  INTEGER { Set (1) }
	ACCESS     read-write
	STATUS     mandatory
	DESCRIPTION 
		"Command for restore settings from Flash-memory."
	::= { commands 3 }

ResetCmd  OBJECT-TYPE
	SYNTAX  INTEGER { Set (1) }
	ACCESS     read-write
	STATUS     mandatory
	DESCRIPTION 
		"Command for reset device."
	::= { commands 4 }

JournalEraseCmd  OBJECT-TYPE
	SYNTAX  INTEGER { Set (1) }
	ACCESS     read-write
	STATUS     mandatory
	DESCRIPTION 
		"Command for clear event journal."
	::= { commands 5 }

ClearStatsCmd  OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS     read-write
	STATUS     mandatory
	DESCRIPTION 
		"Command for clear statistics for ports. Write port type and port number to clear its statistics.   
		bits 15..8 - port type (0 - E1 port, 1 - copper Ethernet port, 2 - STM-1 port);   
		bits 7..0 - port number (0 - 1st, 1 - 2nd, ... 0xFF - all ports)"
	::= { commands 6 }

ResetSettingsCmd  OBJECT-TYPE
	SYNTAX  INTEGER { Set (1) }
	ACCESS     read-write
	STATUS     mandatory
	DESCRIPTION 
		"Command for reset settings to its default (without apply)."
	::= { commands 7 }

SyncCmd  OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS     read-write
	STATUS     mandatory
	DESCRIPTION 
		"Sync Control Commands: 
		bits 15..8 - command type (0 - set lock-out, 1 - clear lock-out, 2 - clear forced and manual switch, 
		                           3 - forced switch, 4 - manual switch; 
		bits 7..0 - sync input (0..1 - STM-1 RCLK, 32..47 - E1 RCLK, 64 - External sync input)"
	::= { commands 8 }

MSP_Cmd  OBJECT-TYPE
	SYNTAX  INTEGER
	ACCESS     read-write
	STATUS     mandatory
	DESCRIPTION 
		"Multiplex Section Protection Control Commands: 
		bits 15..8 - command type (0 - clear, 1 - lock-out of protection, 2 - forced switch, 3 - manual switch; 
		bits 7..0 - path number (0 - working path, 1 - protection path)"
	::= { commands 9 }

