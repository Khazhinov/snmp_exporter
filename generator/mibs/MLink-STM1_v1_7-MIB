STP_PortStatTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF STP_PortStatEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"STP port status table."
            ::= { STPState 7 }

			stp_PortStatEntry OBJECT-TYPE
	            SYNTAX  STP_PortStatEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the STP port status table."
	            INDEX { STP_PortStatIndex }
	            ::= { STP_PortStatTable 1 }

			STP_PortStatEntry ::= SEQUENCE {
				STP_PortStatIndex
					INTEGER,
				STP_PortStatStatus
					INTEGER,
				STP_PortStatRole
					INTEGER,
				STP_PortStatFlags
					INTEGER
				}

			STP_PortStatIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each interface.  Its value ranges between 1 and 5."
	            ::= { stp_PortStatEntry 1 }
			
			STP_PortStatStatus    OBJECT-TYPE
	            SYNTAX  INTEGER {
					Blocking (0),
					Learning (1),
					Forwarding (2)
				}
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" Current state of STP port: 0 - Blocking; 1 - Learning; 2 - Forwarding."
	            ::= { stp_PortStatEntry 2 }

			STP_PortStatRole    OBJECT-TYPE
	            SYNTAX  INTEGER {
					Disabled (0),
					Root (1),
					Designated (2),
					Alternate (3),
					Backup (4)
				}
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" Current role of port: 0 - Disabled; 1 - Root; 2 - Designated; 3 - Alternate; 4 - Backup."
	            ::= { stp_PortStatEntry 3 }

			STP_PortStatFlags    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Different flags of STP port status (bitfield):
						bits 7..2 - reserved; 
						bit1 - Edge port mode (PortFast) (0 - non edge, 1 - edge); 
						bit0 - port STP mode (0 - STP, 1 - RSTP)"
	            ::= { stp_PortStatEntry 4 }


