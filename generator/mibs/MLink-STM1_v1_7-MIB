------------------------
-- 'trap-bind' Directory --
------------------------

trap_GlobalAlarm  OBJECT-TYPE
	SYNTAX  INTEGER {
        Normal (0),
        Major (1),
        Minor (2)
         }
	ACCESS     read-only
	STATUS     mandatory
	DESCRIPTION 
		"Device global alarm status: 0 - Normal; 1 - Major; 2 - Minor; 3 - Major+Minor."
	::= { trap-bind 1 }

trap_EventType  OBJECT-TYPE
	SYNTAX     INTEGER
	ACCESS     read-only
	STATUS     mandatory
	DESCRIPTION 
		"A unique value to identify Event Type: 
		 1 - Copper Ethernet; 
		 2 – Fiber Ethernet; 
		 3 – E1; 
		 4 - System; 
		 5 - Backplane.
     6 - STM-1"
	::= { trap-bind 2 }

trap_IfIndex  OBJECT-TYPE
	SYNTAX     INTEGER
	ACCESS     read-only
	STATUS     mandatory
	DESCRIPTION 
		"A unique value to identify Interface Index in the range of: Copper Ethernet(1..3); 
		E1(1..16); System(0); STM-1(1,2);."
	::= { trap-bind 3 }

trap_IfStatus  OBJECT-TYPE
	SYNTAX     INTEGER
	ACCESS     read-only
	STATUS     mandatory
	DESCRIPTION 
		"A unique value to identify Interface alarm status => 
		0 - normal;
		Copper or Fiber Ethernet: 13 - Ethernet link is down, 33 - Ethernet link is up;
		E1: 1 - LOS, 2 - LOF, 3 - AIS, 4 - RAI, 21 - LOS fallout, 22 - LOF fallout, 23 - AIS fallout, 24 - RAI fallout;
		System: 12 - device start up;
		STM-1: 41 - NO_SFP, 42 - LOS, 43 - LOF, 44 - MS_AIS, 45 - MS_RDI, 46 - AU_AIS, 47 - LOP, 48 - VC_AIS, 49 - VC_UNEQ,
					 50 - LOM, 51 - NO_SFP_CLR, 52 - LOS_CLR, 53 - LOF_CLR, 54 - MS_AIS_CLR, 55 - MS_RDI_CLR, 56 - AU_AIS_CLR, 
					 57 - LOP_CLR, 58 - VC_AIS_CLR, 59 - VC_UNEQ_CLR, 60 - LOM_CLR, 61 - DEG, 62 - DEG_CLR."		
	::= { trap-bind 4 }

