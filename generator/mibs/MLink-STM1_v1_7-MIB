-- 'SyncState' SubDirectory --

Sync_state  OBJECT-TYPE
	SYNTAX  INTEGER {
		NO_SWITCH (0),
		MANUAL_SWITCH_ACT (1),
		FORCED_SWITCH_ACT (2),
		FORCED_FREERUN (3)
	}
	ACCESS  read-only
	STATUS 	mandatory
	DESCRIPTION 
			"Global sync status"
	::= { SyncState 1 }

Sync_QL  OBJECT-TYPE
	SYNTAX  INTEGER {
		QL_UNDEF (0),
		QL_DNU (1),
		QL_SEC (2),
		QL_SSU_B (3),
		QL_SSU_A (4),
		QL_PRC (5)
	}
	ACCESS  read-only
	STATUS 	mandatory
	DESCRIPTION 
			"System Quality level"
	::= { SyncState 2 }

CurrentSyncSource  OBJECT-TYPE
	SYNTAX  INTEGER 
	ACCESS  read-only
	STATUS 	mandatory
	DESCRIPTION 
	        " Current system synchronization source:
			0..1 - STM-1 RCLK sources; 
			32..47 - E1 RCLK sources
			64 - External sync input
			128 - Holdover mode
			255 - internal generator (Free run) "
	::= { SyncState 3 }

CurrentSyncState  OBJECT-TYPE
	SYNTAX  INTEGER 
	ACCESS  read-only
	STATUS 	mandatory
	DESCRIPTION 
	        " State of current system synchronization source (bitfield):
			bits 7..3 - reserved; 
			bit 2 - Reference failure
			bit 1 - Locked
			bit 0 - Holdover "
	::= { SyncState 4 }

SyncIn_StateTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF SyncIn_StatEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"Sync inputs status table"
            ::= { SyncState 5 }

			syncIn_StatEntry OBJECT-TYPE
	            SYNTAX  SyncIn_StatEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in sync inputs status table"
	            INDEX { SyncIn_StatIndex }
	            ::= { SyncIn_StateTable 1 }

			SyncIn_StatEntry ::= SEQUENCE {
				SyncIn_StatIndex
					INTEGER,
				SyncIn_Status
					INTEGER
				}

			SyncIn_StatIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for input.  Its value ranges between 1 and 4."
	            ::= { syncIn_StatEntry 1 }

			SyncIn_Status    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Status of sync input (bitfield):
						bit 7 - reserved; 
						bit 6 - lock-out flag;
						bit 5 - reserved; 
						bit 4 - signal fail flag; 
						bits 3..0 - QL of sync input "
	            ::= { syncIn_StatEntry 2 }


---------------------------
-- 'EventJournal' SubDirectory --

EventsTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF EventEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"Event journal table"
            ::= { EventJournal 1 }

			eventEntry OBJECT-TYPE
	            SYNTAX  EventEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in event journal"
	            INDEX { EventIndex }
	            ::= { EventsTable 1 }

			EventEntry ::= SEQUENCE {
				EventIndex
					INTEGER,
				EventElement
					OCTET STRING
				}

			EventIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Event number. Maximum number of events in journal is 1024. 
						But really number of events depend on actually occured events"
	            ::= { eventEntry 1 }

			EventElement    OBJECT-TYPE
	            SYNTAX  OCTET STRING (SIZE(16))
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Event entry. One entry contain 16 bytes:
						bytes 0..5 - time of event; 
						byte 6 - type of event (0 - Power down, 1 - E1 port event, 3 - Start working,
								5 - copper Ethernet port event, 6 - fiber Ethernet port event); 
						byte 7 - number of port for appropriate event type (for E1 events - 1..8,
								for copper or fiber Ethernet events - 1..2); 
						byte 8 - current state of port; 
						byte 9 - previous state of port; 
						bytes 10..15 - reserved "
	            ::= { eventEntry 2 }
			

