LANStatTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF LAN_EthStatEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"Status and statistics table for LAN Ethernet ports"
            ::= { EthState 1 }

			lan_EthStatEntry OBJECT-TYPE
	            SYNTAX  LAN_EthStatEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the LAN Ethernet status and statistics table."
	            INDEX { LAN_EthStatIndex }
	            ::= { LANStatTable 1 }

			LAN_EthStatEntry ::= SEQUENCE {
				LAN_EthStatIndex
					INTEGER,
				LAN_EthStatStatus
					INTEGER,
				LAN_EthStatRxGoodOctets
					Counter,
				LAN_EthStatRxBadOctets
					Counter,
				LAN_EthStatRxFrames
					Counter,
				LAN_EthStatRxErrors
					Counter,
				LAN_EthStatTxOctets
					Counter,
				LAN_EthStatTxFrames
					Counter
				}

			LAN_EthStatIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each port.  Its value ranges between 1 and number of LAN ports."
	            ::= { lan_EthStatEntry 1 }

			LAN_EthStatStatus    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Current status of LAN Ethernet port (bitfield):
						bits 15..6 - reserved; 
						bit 5 - MDI crossover status (0 - MDI, 1 - MDIX); 
						bit 4 - ANeg status (0 - ANeg not complete, 1 - ANeg complete); 
						bit 3 - Link status (0 - Link Down, 1 - Link Up);
						bit 2 - Duplex (0 - half, 1 - full);
						bits 0..1 - Speed (00 - 10Mbps, 01 - 100Mbps, 10, 11 - reserved)"
	            ::= { lan_EthStatEntry 2 }
			
			LAN_EthStatRxGoodOctets    OBJECT-TYPE
	            SYNTAX  Counter
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Number of octets received without errors "
	            ::= { lan_EthStatEntry 3 }

			LAN_EthStatRxBadOctets    OBJECT-TYPE
	            SYNTAX  Counter
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Number of errored octets "
	            ::= { lan_EthStatEntry 4 }

			LAN_EthStatRxFrames    OBJECT-TYPE
	            SYNTAX  Counter
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Number of received frames"
	            ::= { lan_EthStatEntry 5 }

			LAN_EthStatRxErrors    OBJECT-TYPE
	            SYNTAX  Counter
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Number of errors for Rx"
	            ::= { lan_EthStatEntry 6 }

			LAN_EthStatTxOctets    OBJECT-TYPE
	            SYNTAX  Counter
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Number of transmitted octets"
	            ::= { lan_EthStatEntry 7 }

			LAN_EthStatTxFrames    OBJECT-TYPE
	            SYNTAX  Counter
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Number of transmitted frames"
	            ::= { lan_EthStatEntry 8 }

WANStatTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF WAN_EthStatEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"Status table for WAN Ethernet ports"
            ::= { EthState 2 }

			wan_EthStatEntry OBJECT-TYPE
	            SYNTAX  WAN_EthStatEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the WAN Ethernet ports status table."
	            INDEX { WAN_EthStatIndex }
	            ::= { WANStatTable 1 }

			WAN_EthStatEntry ::= SEQUENCE {
				WAN_EthStatIndex
					INTEGER,
				WAN_EthStatNumber_of_VC12
					INTEGER,
				WAN_EthStatLinkState
					INTEGER,
				WAN_EthStatGFPState
					INTEGER
				}

			WAN_EthStatIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each port.  Its value ranges between 1 and number of WAN ports."
	            ::= { wan_EthStatEntry 1 }

			WAN_EthStatNumber_of_VC12    OBJECT-TYPE
	            SYNTAX  INTEGER 
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" Number of VC12 used for WAN port"
	            ::= { wan_EthStatEntry 2 }

			WAN_EthStatLinkState    OBJECT-TYPE
	            SYNTAX  INTEGER 
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Current link state of WAN port (bitfield):
						bits 7..1 - reserved; 
						bit 0 - Link state (0 - down, 1 - up)"
	            ::= { wan_EthStatEntry 3 }

			WAN_EthStatGFPState    OBJECT-TYPE
	            SYNTAX  INTEGER 
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"Current state of GFP decapsulation module (bitfield):
						bit 12 - flag of GFP SYNC state; 
						bit 9 - flag of GFP PRESYNC state; 
						bit 5 - flag of GFP HUNT state"
	            ::= { wan_EthStatEntry 4 }
			

WAN_VC12_StatTable 	OBJECT-TYPE
			SYNTAX  SEQUENCE OF WAN_VC12_StatEntry
            ACCESS  not-accessible
            STATUS  mandatory
            DESCRIPTION
            		"Status table for VC12 "
            ::= { EthState 3 }

			wan_VC12_StatEntry OBJECT-TYPE
	            SYNTAX  WAN_VC12_StatEntry
	            ACCESS  not-accessible
	            STATUS  mandatory
	            DESCRIPTION
	            		"An entry in the VC12 status table."
	            INDEX { WAN_VC12_StatIndex }
	            ::= { WAN_VC12_StatTable 1 }

			WAN_VC12_StatEntry ::= SEQUENCE {
				WAN_VC12_StatIndex
					INTEGER,
				WAN_VC12_Mapping
					INTEGER,
				WAN_VC12_VCAT_State
					INTEGER,
				WAN_VC12_Trib_State
					INTEGER,
				WAN_VC12_RxVCAT_State
					INTEGER,
				WAN_VC12_TxVCAT_State
					INTEGER
				}

			WAN_VC12_StatIndex    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		"A unique value for each VC12.  Its value ranges between 1 and 63."
	            ::= { wan_VC12_StatEntry 1 }

			WAN_VC12_Mapping    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" Mapping of VC12 to STM-1 ports (bitfield):
						bit 15 - active flag (0 - idle; 1 - active); 
						bits 14..12 - reserved;
						bits 11..8 - number of STM (0..1); 
						bits 7..6 - reserved;  
						bits 5..0 - number of VC12 (0..62) "
	            ::= { wan_VC12_StatEntry 2 }

			WAN_VC12_VCAT_State    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" VCAT state of VC12:
						0 - Idle; 
						1 - Adding; 
						2 - Active "
	            ::= { wan_VC12_StatEntry 3 }
			
			WAN_VC12_Trib_State    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" State of VC12 tributary (bitfield):
						 bit14 - LOP; 
						 bit13 - TU-AIS; 
						 bit10 - K4-LOM; 
						 bit6 - V5-RDI; 
						 bit4 - V5-UNEQ; 
						 bit3 - V5-PLM; 
						 bit2 - V5-AIS "
	            ::= { wan_VC12_StatEntry 4 }

			WAN_VC12_RxVCAT_State    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" Rx VCAT state for VC12 (bitfield):
						 bits 14..9 - sequence indicator value; 
						 bits 8..5 - CTRL field value "
	            ::= { wan_VC12_StatEntry 5 }

			WAN_VC12_TxVCAT_State    OBJECT-TYPE
	            SYNTAX  INTEGER
	            ACCESS  read-only
	            STATUS  mandatory
	            DESCRIPTION
	            		" Tx VCAT state for VC12 (bitfield):
						 bits 14..9 - sequence indicator value; 
						 bits 8..5 - CTRL field value; 
						 bit0 - FAIL flag (for LCAS mode) "
	            ::= { wan_VC12_StatEntry 6 }


